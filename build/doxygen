#!/bin/bash

readonly service_cfg=${0%/scripts*}/etc/service.cfg

. ${service_cfg} && . ${scripts_base}/common

log "${info} Installing doxygen"

apt-get install -y doxygen
if [ $? -eq 0 ]; then
    log "${success} doxygen installed"
    exit 0
else
    log "${critical} problem installing doxygen"
    exit 2
fi



