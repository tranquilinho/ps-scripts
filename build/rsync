#!/bin/bash

readonly service_cfg=${0%/scripts*}/etc/service.cfg

. ${service_cfg} && . ${scripts_base}/common

log "${info} Installing rsync"

apt-get install -y rsync
if [ $? -eq 0 ]; then
    log "${success} rsync installed"
    exit 0
else
    log "${critical} problem installing rsync"
    exit 2
fi



