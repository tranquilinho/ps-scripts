#!/usr/bin/env bash

readonly service_cfg=${0%/scripts*}/etc/service.cfg

. ${service_cfg} && . ${scripts_base}/common

readonly pkg_name=gsl
readonly pkg_version=2.1
readonly url_base=http://ftp.rediris.es/mirror/GNU/gnu/gsl

build_preconditions

export PATH=${usr_prefix}/pkg-config/bin:${PATH}

export CC=${usr_prefix}/gcc/bin/gcc
./configure --prefix=${prefix}   

standard_install

